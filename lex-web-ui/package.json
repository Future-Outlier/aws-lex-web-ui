{
  "name": "lex-web-ui",
  "version": "0.23.0",
  "description": "Amazon Lex Web Interface",
  "author": "AWS",
  "license": "Amazon Software License",
  "private": true,
  "type": "module",
  "main": "dist/bundle/lex-web-ui.js",
  "scripts": {
    "serve": "cross-env vite",
    "dev": "cross-env vite",
    "start": "cross-env vite",
    "build": "cross-env vite build",
    "build:dev": "cross-env NODE_ENV=development vite build",
    "build-dist": "npm run build:lib-dev && npm run build:lib-prod",
    "build:lib-dev": "cross-env BUILD_TARGET=lib NODE_ENV=development vite build && cross-env BUILD_TARGET=lib NODE_ENV=development node scripts/post-build-css.js",
    "build:lib-prod": "cross-env BUILD_TARGET=lib NODE_ENV=production vite build && cross-env BUILD_TARGET=lib NODE_ENV=production node scripts/post-build-css.js",
    "lint": "eslint src --ext .vue,.js,.jsx,.ts,.tsx",
    "test:integration": "jest --config test/integration/jest.config.js"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.965.0",
    "@aws-sdk/client-connect": "^3.965.0",
    "@aws-sdk/client-lex-runtime-v2": "^3.965.0",
    "@aws-sdk/client-polly": "^3.965.0",
    "@aws-sdk/client-s3": "^3.965.0",
    "@aws-sdk/credential-providers": "^3.965.0",
    "amazon-connect-chatjs": "^2.3.0",
    "aws-amplify": "^5.3.26",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "core-js": "^3.6.5",
    "jwt-decode": "^4.0.0",
    "marked": "^4.0.12",
    "material-design-icons": "^3.0.1",
    "roboto-fontface": "^0.10.0",
    "stream-browserify": "^3.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.2.5",
    "vuetify": "^3.8.3",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^5.2.4",
    "cross-env": "^10.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^9.19.2",
    "jest": "^30.2.0",
    "sass": "^1.97.2",
    "vite": "^5.4.21",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-vuetify": "^2.1.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "postcss": "8.4.32"
  }
}
